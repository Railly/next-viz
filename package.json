{
  "name": "react-aleph-root",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "index.js",
  "workspaces": [
    "lib/**"
  ],
  "scripts": {
    "start:cli": "npm run start --workspace=lib/cli",
    "start:core": "npm run start --workspace=lib/core",
    "dev:client": "npm run dev --workspace=lib/client",
    "test:cli": "npm run test --workspace=lib/cli",
    "test:core": "npm run test --workspace=lib/core",
    "test": "vitest",
    "coverage": "vitest run --coverage",
