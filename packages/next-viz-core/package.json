{
  "name": "@next-viz/core",
  "type": "module",
  "version": "0.0.3",
  "description": "Next Viz Core",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
