{
  "name": "@next-viz/api",
  "type": "module",
  "version": "0.0.1",
  "description": "Next Viz API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "author": "Railly Hugo",
  "license": "ISC",
  "scripts": {
    "start": "nodemon --watch src/**/*.ts --exec \"ts-node --esm\" src/index.ts",
    "build": "tsup",
    "build:tsc": "tsc -p .",
    "precommit": "echo '@next-viz/api'"
  },
  "dependencies": {
    "@next-viz/core": "*",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "express": "4.18.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "17.0.45",
    "nodemon": "2.0.19",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "bugs": {
    "url": "https://github.com/Railly/next-viz/issues"
  },
  "homepage": "https://github.com/Railly/next-viz/tree/main/packages/next-viz-api#readme"
}
