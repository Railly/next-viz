  "keywords": [
    "core",
    "parser",
    "performance"
  ],
  "devDependencies": {
    "@swc/cli": "0.1.57",
    "@swc/core": "1.2.204",
    "@types/eslint": "^8.4.6",
    "tsup": "^6.3.0"
  },
  "author": "Railly Hugo",
  "license": "ISC",
