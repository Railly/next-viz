{
  "name": "react-aleph-core",
  "version": "0.0.1",
  "description": "React Aleph Official Core",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "start": "tsc && node --es-module-specifier-resolution=node ./dist/index.js",
    "build": "tsup src/index.ts --format esm,cjs --dts",
