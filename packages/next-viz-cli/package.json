{
  "name": "@next-viz/cli",
  "version": "0.0.0",
  "description": "Next Viz CLI",
  "main": "./dist/index.js",
  "bin": {
    "next-viz": "./dist/index.js",
    "precommit": "echo '@next-viz/cli'"
  },
  "scripts": {
    "start": "nodemon --watch src/**/*.ts --exec \"ts-node --esm\" src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsup src/index.ts --format esm,cjs --dts dist/index.js",
    "build:tsc": "tsc -p .",
    "test": "test",
    "local": "npm i -g && react-aleph-cli",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm i",
    "publish": "npm publish",
    "precommit": "echo 'react-aleph-cli' precommit"
  },
  "keywords": [
    "next-viz",
    "visualization",
    "performance",
    "statistics"
  ],
  "author": "Railly Hugo",
  "license": "ISC",
  "dependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "chalk": "4.1.2",
    "clear": "0.1.0",
    "commander": "9.3.0",
    "commitizen": "^4.2.5",
    "figlet": "1.5.2",
    "gradient-string": "^2.0.2",
    "path": "0.12.7"
  },
  "devDependencies": {
    "@types/gradient-string": "^1.1.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsup": "^6.3.0"
  }
}
