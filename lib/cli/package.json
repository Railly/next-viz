{
  "name": "react-aleph-cli",
  "version": "0.0.1",
  "description": "React Aleph Official CLI",
  "type": "module",
  "main": "./lib/index.js",
  "bin": {
    "react-aleph-cli": "./lib/index.js"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "start": "nodemon --watch src/**/*.ts --exec \"ts-node --esm\" src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "test",
    "local": "npm i -g && react-aleph-cli",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm i",
    "publish": "npm publish"
  },
  "keywords": [
    "react-aleph",
    "visualization",
    "performance",
    "statistics"
  ],
  "author": "Railly Hugo",
  "license": "ISC",
  "dependencies": {
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "chalk": "4.1.2",
    "clear": "0.1.0",
    "commander": "9.3.0",
    "figlet": "1.5.2",
    "path": "0.12.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
